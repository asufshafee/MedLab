<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e8eaf7"
    android:orientation="vertical"
    tools:context="com.webmarke8.app.medlab.Activities.MainActivity">

    <include
        android:id="@+id/toolbar_with_search"
        layout="@layout/toolbar"></include>

    <include
        android:id="@+id/toolbar_with_back"
        layout="@layout/toolbar_with_back"
        android:visibility="invisible"></include>

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar_with_search"
        android:layout_marginBottom="@dimen/_30sdp" />

    <include layout="@layout/tabs_layout"></include>


</RelativeLayout>
